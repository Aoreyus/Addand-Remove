  <body>
    <h1>Room 1</h1>
    <script>
    function addAnother(){
      
      var addGuess= document.createElement('li');
      var putGuess= document.createTextNode(A.value);
      
      addGuess.appendChild(putGuess);
      
      var lockGuess = document.getElementsByTagName('ul')[0];
      
      lockGuess.appendChild(addGuess);
    }
    function removeOne(){
        var y =(document.getElementById('B').value - 1);
        var stupidKid =document.getElementsByTagName('li')[y];
        var hana =stupidKid.parentNode;
        hana.removeChild(stupidKid);
      }
      
    </script>
    <input type='text'id="A">
    <button onclick="addAnother()">
    Check In
    </button>
    <br>
    <input type='number'id="B">
    <button onclick="removeOne()">
    Check Out </button>
